    y =y+sign(vsp)
    }
vsp = 0
}
if (grid_place_meeting(x+hsp,y)/*place_meeting(x+hsp,y,obj_wall)*/)
{
    while (!grid_place_meeting(x+sign(hsp),y)/*!place_meeting(x+sign(hsp),y,obj_wall)*/)
    {
    x=x+sign(hsp)
    }
hsp = 0
}
x +=hsp;
y +=vsp;
